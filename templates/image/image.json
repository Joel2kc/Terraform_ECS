[
    {
      "name": "testapp",
      "image": "nginx:latest",
      "cpu": 1024,
      "memory": 3072,
      "networkMode": "awsvpc",
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/testapp",
            "awslogs-region": "eu-west-2",
            "awslogs-stream-prefix": "ecs"
          }
      },
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ]